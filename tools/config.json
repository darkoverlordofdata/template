{
  "packages": {
  },
  "version": {
    "template": "build.ts.tpl",
    "source": "build.ts",
    "path": "./lib/src",
    "key": "VERSION"
  },
  "build": {
    "library": false,
    "compile": true,
    "dest": "build",
    "sources": [
      "web/src/**/*.*"
    ],
    "resources": [
      "lib/res/**/*.*"
    ],
    "files": [
      "web/res/**/*.*",
      "web/index.html",
      "web/license.md",
      "web/readme.md"
    ]
  },
  "deploy": {
    "path": "web/frameworks/runtime-src/alienzone/web/src/main/assets/",
    "skip": 2,
    "extra": [
      "build/web/res/**/*.*",
      "build/web/index.html",
      "build/web/license.md",
      "build/web/main.js",
      "build/web/project.json",
      "build/web/readme.md"
    ]
  },
  "patch": {}
}

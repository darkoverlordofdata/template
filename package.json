{
  "name": "Template.coffee",
  "version": "0.0.1",
  "description": "{{ description }}",
  "main": "index.js",
  "scripts": {
    "_vscode_build": "tsc --watch",
    "android": "",
    "build": "",
    "clean": "rm -rf build/*",
    "cocos": "cake cocos",
    "compile": "python  /home/bruce/Applications/closure/library/closure/bin/build/closurebuilder.py --root=/home/bruce/Applications/closure/library/ --root=./goog/lib --root=./goog/example --input=./goog/example/index.js --namespace=asteroids --output_mode=compiled --compiler_jar=/home/bruce/Applications/closure/compiler/compiler.jar --compiler_flag='--compilation_level=ADVANCED_OPTIMIZATIONS' --compiler_flag='--define=goog.userAgent.ASSUME_WEBKIT=true' --compiler_flag='--create_source_map=web/ash.js.map' --compiler_flag='--warning_level=QUIET' --compiler_flag='--language_in=ECMASCRIPT5' > web/ash.min.js",
    "config":"cake prebuild:make",
    "convert": "coffee tools/convert.coffee",
    "deploy": "cp -rf web/res web/frameworks/runtime-src/proj.android-studio/app/assets && cp -f web/main.js web/frameworks/runtime-src/proj.android-studio/app/assets && cp -f web/project.json web/frameworks/runtime-src/proj.android-studio/app/assets && cp -rf web/src web/frameworks/runtime-src/proj.android-studio/app/assets",
    "folders": "mkdir build/web && mkdir build/lib && cp -fr lib/src build/lib && cp -fr web/res build/web",
    "get": "bower-installer && cake get:patch",
    "jsdoc": "jsdoc goog/lib -r --template ../jaguarjs-jsdoc --configure ./conf.json --readme ./readme.md --destination ./build/web",
    "manifest": "gulp manifest:appcache",
    "postbuild": "cake postbuild:update",
    "postget": "cp packages/closure-compiler/lib/vendor/compiler.jar ./tools",
    "postinstall": "bower install && npm run get",
    "preandroid": "cake preandroid:make && npm run predeploy && npm run transpile && npm run resources && cp -fr web/res ./web/frameworks/runtime-src/proj.android-studio/app/assets",
    "prebuild": "cake prebuild:make && npm run clean -s && npm run transpile && npm run resources && npm run folders",
    "predeploy": "rm -rf web/frameworks/runtime-src/proj.android-studio/app/assets/res && rm -rf web/frameworks/runtime-src/proj.android-studio/app/assets/src && rm -f web/frameworks/runtime-src/proj.android-studio/app/assets/main.js && rm -f web/frameworks/runtime-src/proj.android-studio/app/assets/project.json",
    "preget": "rm -rf web/src/jmatch3/*.* && rm -rf web/src/tween.ts/*.*",
    "publish": "gulp publish:gh-pages",
    "resources": "cp -rf lib/res web/res",
    "start": "node ./tools/server.js",
    "serve": "node ./tools/server.js build/web",
    "test": "NODE_ENV=test mocha --reporter nyan",
    "transpile": "tsc",
    "version": "cake version:bump"
  },
  "author": "{{ author }}",

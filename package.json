{
  "name": "{{ name }}",
  "version": "0.0.1",
  "description": "{{ description }}",
  "main": "index.js",
  "config": {},
  "scripts": {
    "_vscode_build": "entitas generate && tsc -p ./tsconfig.json",
    "build": "tsc -p . --outFile build/{{ name }}.js -d && cat build/{{ name }}.js | java -jar packages/closure-compiler/lib/vendor/compiler.jar --compilation_level WHITESPACE_ONLY --js_output_file build/{{ name }}.min.js",
    "clean": "rm -rf build/*",
    "doc": "typedoc --out build/web lib{{ name }} --mode file --theme node_modules/typedoc-dash-theme/bin",
    "publish": "gulp publish",
    "postinstall": "bower-installer",
    "prebuild": "npm run clean -s",
    "start": "tools/server web",
    "serve": "tools/server build/web",
    "test": "NODE_ENV=test mocha --compilers coffee:coffee-script --require test/test_helper.js --recursive"
  },
  "author": "{{ author }}",
  "license": "MIT",
  "directories": {
    "lib": "./lib",
    "example": "./example"
  },
  "devDependencies": {
    "chai": "~1.9.2",
    "coffee-script": "1.9.0",
    "gulp": "^3.9.0",
    "gulp-gh-pages": "^0.5.2",
    "gulp-shell": "^0.4.2",
    "gulp-webserver": "^0.9.1",
    "liquid.coffee": "^0.1.1",
    "mocha": "~2.0.1",
    "morgan": "^1.6.1",
    "nw": "^0.17.3",
    "walker": "^1.0.7"
  },
  "dependencies": {
  }
}
